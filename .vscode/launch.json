{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Extension Host",
            "type": "extensionHost",
            "cwd": "${workspaceFolder}/vscode-extension",
            "request": "launch",
            "preLaunchTask": "build-extension",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}/vscode-extension",
                "${workspaceFolder}/vscode-extension/example"
            ]
        }
    ]
}